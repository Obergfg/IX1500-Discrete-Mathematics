  RowBox[{
   RowBox[{
    RowBox[{"Binomial", "[", 
     RowBox[{"5", ",", "1"}], "]"}], "*", 
    RowBox[{
     RowBox[{"Binomial", "[", 
      RowBox[{"4", ",", "1"}], "]"}], "^", "5"}]}], "-", 
   RowBox[{
    RowBox[{"Binomial", "[", 
     RowBox[{"5", ",", "1"}], "]"}], "*", 
    RowBox[{"Binomial", "[", 
     RowBox[{"4", ",", "1"}], "]"}]}]}]}]], "Input",
